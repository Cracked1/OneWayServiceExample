  "jest": {
    "browser": true,
    "moduleDirectories": [
      "<rootDir>/WebSite/redesign/js/alumni",
      "<rootDir>/WebSite/redesign/js",
      "node_modules"
    ],
    "collectCoverageFrom": [
      "WebSite/redesign/js/alumni/components/**/*.js",
      "WebSite/redesign/js/alumni/hoc/forms/Form.js",
      "WebSite/redesign/js/alumni/helper/GeocoderExtension.js",
      "WebSite/redesign/js/alumni/hoc/BrowserWarning.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 75,
        "lines": 75,
        "statements": 0
      }
    },
    "coveragePathIgnorePatterns": [
      "<rootDir>/WebSite/redesign/js/alumni/scripts",
      "node_modules",
      "<rootDir>/WebSite/redesign/js/alumni/components/forms/Captcha.js",
      "<rootDir>/WebSite/redesign/js/alumni/components/ExampleForm.js",
      "<rootDir>/WebSite/redesign/js/alumni/components/GoogleZipCodeExample.js",
      "<rootDir>/WebSite/redesign/js/alumni/components/PermissionsModifier.js",
      "<rootDir>/WebSite/redesign/js/alumni/components/TestAutoComplete.js",
      "<rootDir>/WebSite/redesign/js/alumni/components/ExampleModal.js",
      "<rootDir>/WebSite/redesign/js/alumni/components/ExampleToolTip.js",
      "<rootDir>/WebSite/redesign/js/alumni/components/AuthenticatedContact.js",
      "<rootDir>/WebSite/redesign/js/alumni/components/common/SearchResult.js"
    ],
    "coverageDirectory": "<rootDir>/WebSite/redesign/js/coverage-report",
    "coverageReporters": ["text", "lcov"],
    "roots": [
      "<rootDir>/WebSite/redesign/js/alumni/__tests__",
      "<rootDir>/WebSite/redesign/js/alumni"
    ],
    "testMatch": ["**/__tests__/**/*.spec.js"],
    "verbose": true,
    "setupFiles": [
      "<rootDir>/WebSite/redesign/js/alumni/scripts/tests/globals.js",
      "<rootDir>/WebSite/redesign/js/alumni/scripts/tests/jestsetup.js"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "snapshotSerializers": ["<rootDir>/node_modules/enzyme-to-json/serializer"]
  },
  "scripts": {
    "dev": "yarn install && ./node_modules/.bin/gulp dev --env development",
    "prod": "./node_modules/.bin/gulp prod --env production",
    "post-prod": "./node_modules/.bin/gulp post-prod --env production",
    "test": "./node_modules/.bin/jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepush": "yarn run test"
